{
  "name": "react-flask-app",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^1.3.4",
    "concurrently": "^6.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "4.0.3",
    "react-router-dom": "^6.0.0",
    "yarn": "^1.22.10",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"yarn start-api\"",
    "start-api": "cd server && cross-env-shell 'bash -c \"if [ \\\"$OS\\\" = \\\"Windows_NT\\\" ]; then .\\\\venv\\\\Scripts\\\\activate; else source venv/bin/activate; fi && flask run --no-debugger\"'",
    "build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
